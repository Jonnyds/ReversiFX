<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="root" spacing="10" xmlns="http://javafx.com/javafx/8.0.0"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ReversiController">
        <children>
                <VBox id="VBOX" fx:id="VBOX" spacing="10">
                        <children>
                                <Button id="startButton" text="Start Game">Start Game</Button>
                                <Button id="settingButton" text="Settings">Setting</Button>
                                <Button id="endGameButton" text="End Game">End Game</Button>
                        </children>
                </VBox>
        </children>
</HBox>